package tp6.ex01;

import java.util.ArrayList;

public class Team {

	String name;
	ArrayList<TeamMember> members;
	
	public Team(String name) {
		this.name = name;
		members = new ArrayList<TeamMember>();
	}
	
	public void addMembers(TeamMember member) {
		members.add(member);
	}
	
	@Override
	public String toString() {
		String res = name + " [";
		for (TeamMember m : members) {
			res += " " + m.toString();
		}
		return res + "]";
	}
}
