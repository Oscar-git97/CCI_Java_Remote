package tp7.exo2;

import java.math.BigInteger;

public class Calculator {

	public int powOf10(int n) {
		return (int) Math.pow(10, n);
	}

	public BigInteger powOf10(BigInteger n) {
		
		BigInteger pow = BigInteger.TEN;
		
		for (BigInteger i = BigInteger.ZERO; i.compareTo(n) == -1; i.add(BigInteger.ONE))
			pow = pow.multiply(pow);
		return pow;
	}
	
	
}
