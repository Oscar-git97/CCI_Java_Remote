package tp7.exo2;

import java.math.BigInteger;

public class Calculator {

	public int powOf10(int n) {
		return (int) Math.pow(10, n);
	}

	public BigInteger powOf10(BigInteger n) {
		
		BigInteger test = new BigInteger("0");
		
		for (BigInteger i = ; i < n; i++)
			return (int) Math.pow(10, n);
	}
}
