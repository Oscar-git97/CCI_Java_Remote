package td7;

public class Point {
	String name;
	double x;
	double y;
	
	public Point() {
		this(null, 0, 0);
	}
	
	public Point(String name, double x, double y) {
		this.name = name;
		this.x = x;
		this.y = y;
	}
	
	public Point scale(double k) {
		return new Point();
	}
	
	
	
	@Override
	public String toString() {
		return name+"("+x+";"+y+")";
	}

}
