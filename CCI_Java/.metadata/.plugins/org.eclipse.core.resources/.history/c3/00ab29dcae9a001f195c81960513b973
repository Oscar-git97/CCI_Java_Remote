package tp7.exo2;

public class TestPowOf10V2 {

	public static void main(String[] args) {
	System.out.println("10^0 = " + Calculator.powOf10(0));
	System.out.println("10^1 = " + Calculator.powOf10(1));
	System.out.println("10^2 = " + Calculator.powOf10(2));
	System.out.println("10^3 = " + Calculator.powOf10(3));
	}

}
